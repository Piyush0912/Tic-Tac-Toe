<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#022642"
    tools:context=".MainActivity2">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="72dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.391"
        app:srcCompat="@drawable/vs" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="150dp"
        android:layout_height="109dp"
        android:gravity="center"
        android:textColor="#FF5722"
        android:textSize="34sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.372" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="cursive"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="Tic Tac Toe"
        android:textColor="#EF0000"
        android:textColorHighlight="#F8BB04"
        android:textColorLink="#0C2BF8"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="150dp"
        android:layout_height="109dp"
        android:gravity="center"
        android:textColor="#FF5722"
        android:textSize="34sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.383" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="420dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.822">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="135dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:width="5px"
                android:tag="0"
                android:textColor="#020202"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:cornerRadius="3px"
                app:iconPadding="8dp"
                app:iconTint="#454040"
                app:iconTintMode="add"
                app:rippleColor="#FFF0F0"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

            <Button
                android:id="@+id/button2"
                android:layout_width="86dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="1"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

            <Button
                android:id="@+id/button3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="2"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="142dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="3"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

            <Button
                android:id="@+id/button5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="4"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

            <Button
                android:id="@+id/button6"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="5"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="137dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button7"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="6"
                android:textColor="#020202"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="7"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

            <Button
                android:id="@+id/button9"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:tag="8"
                android:textColor="#000000"
                android:textSize="48sp"
                android:textStyle="bold"
                app:backgroundTint="@android:color/holo_orange_light"
                app:strokeColor="#050505"
                app:strokeWidth="15px" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>